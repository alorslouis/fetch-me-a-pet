{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "types": ["@cloudflare/workers-types/experimental"],
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "strictNullChecks": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": ["src/components/*"],
      "@partials/*": ["src/pages/partials/*"],
      "@typedef/*": ["src/appTypes/*"],
      "@pages/*": ["src/pages/*"],
      "@api/*": ["src/pages/api/*"],
      "@db/*": ["db/*"],
      "@utils/*": ["utils/*"],
      "@layouts/*": ["src/layouts/*"]
    }
  }
}
